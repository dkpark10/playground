{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "commonjs",
    "noEmit": true,
    "strictPropertyInitialization": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": "../react-query",
    "paths": {
      "@/*": [
        "../react-query/src/*"
      ]
    },
    "plugins": [
      {
        "name": "next",
      },
      {
        "name": "typescript-plugin-css-modules",
      }
    ]
  },
  "include": [
    "../react-query/next-env.d.ts",
    "../react-query/**/*.ts",
    "../react-query/**/*.tsx",
    "../react-query/@types",
    "../react-query/graffiti.js",
    "../react-query/.next/types/**/*.ts"
  ],
  "exclude": [
    "../react-query/node_modules",
    "../react-query/cypress",
    "../react-query/cypress.config.ts",
  ]
}